{
    "name": "registry-app",
    "version": "0.1.0",
    "proxy": "http://localhost:5000",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "concurrently": "^4.1.0",
        "dotenv": "^8.1.0",
        "http-proxy-middleware": "^0.19.1",
        "node-sass": "^4.11.0",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.5",
        "react-scrollable-anchor": "^0.6.1",
        "universal-cookie": "^3.0.7",
        "validator": "^10.11.0"
    },
    "scripts": {
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "db:init": "dropdb --if-exists registry_db && createdb registry_db",
        "db:reset": "node scripts/resetDb.js",
        "db:model": "node models/models.js",
        "db:seed": "node scripts/seed.js",
        "server": "nodemon server.js",
        "dev": "concurrently \"npm run server\" \"react-scripts start\"",
        "start2": "concurrently \"node server.js\" \"react-scripts start\"",
        "start": "node server.js",
        "heroku-postbuild": "npm run build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
